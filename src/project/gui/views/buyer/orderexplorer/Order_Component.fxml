<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListCell?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<fx:root prefHeight="100.0" prefWidth="600.0" type="ListCell" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1">
	<graphic>
		<AnchorPane minHeight="150.0" prefWidth="700.0">
         <children>
            <Label fx:id="lblOrderID" layoutX="14.0" layoutY="5.0" text="{ORDER ID}">
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
            <Label fx:id="lblOrderCreationDate" layoutX="15.0" layoutY="30.0" text="{ORDER DATE}">
               <font>
                  <Font size="12.0" />
               </font>
            </Label>
            <Label fx:id="lblOrderStatus" layoutX="480.0" layoutY="5.0" text="{ORDER STATUS}">
               <font>
                  <Font size="12.0" />
               </font>
            </Label>
            <Label fx:id="lblOrderPrice" layoutX="368.0" layoutY="20.0" text="{ORDER PRICE}">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Label layoutX="15.0" layoutY="58.0" text="Products:" />
            <Label layoutX="227.0" layoutY="94.0" text="NOT IMPLEMENTED YET" />
         </children>
		</AnchorPane>
	</graphic>
</fx:root>
